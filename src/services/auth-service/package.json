{
  "name": "@emul8/auth-service",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
